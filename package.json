{
	"name": "bleek-dot-dev",
	"version": "0.5.0",
	"main": "src/pages/_app.tsx",
	"type": "module",
	"author": {
		"name": "Brandon Leek",
		"url": "https://github.com/bleek42/bleek-dot-dev"
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=8",
		"vscode": "^1.22.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write",
		"lntfmt": "next lint src/**/*.{ts,tsx} && prettier --write src/**/*.{ts,tsx}",
