{
	"name": "bleek-dot-dev",
	"version": "0.5.0",
	"type": "module",
	"author": {
		"name": "Brandon Leek",
		"url": "https://github.com/bleek42/bleek-dot-dev"
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=8",
		"vscode": "^1.22.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"tunnel": "npm run dev && ngrok http 3000",
		"format": "prettier --write",
		"lntfmt": "next lint src/**/*.{ts,tsx} && prettier --write src/**/*.{ts,tsx}",
		"gen": "graphql-codegen --require dotenv/config --config codegen.ts",
		"gen:watch": "graphql-codegen --require dotenv/config --config --watch codegen.ts",
		"gen:rm": "rm -rf src/types/graphql/*.ts",
		"deps:rm": "rm -rf node_modules/ pnpm-lock.yaml"
	},
	"dependencies": {
		"dotenv": "16.1.4",
		"graphql": "^16.7.1",
		"graphql-request": "^6.1.0",
		"graphql-tag": "^2.12.6",
		"next": "^13.4.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^5.3.1"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^4.0.1",
		"@graphql-codegen/client-preset": "^4.0.1",
		"@graphql-codegen/client-preset-swc-plugin": "0.2.0",
		"@graphql-codegen/typed-document-node": "^5.0.1",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-codegen/typescript-graphql-request": "5.0.0",
		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@graphql-eslint/eslint-plugin": "3.19.1",
		"@graphql-typed-document-node/core": "3.2.0",
		"@styled/typescript-styled-plugin": "1.0.0",
		"@types/node": "^20.3.3",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"eslint": "^8.44.0",
		"eslint-config-next": "^13.4.8",
		"eslint-config-prettier": "8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-i": "2.27.5",
		"eslint-plugin-n": "16.0.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"graphql-scalars": "^1.22.2",
		"prettier": "2.8.8",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"optionalDependencies": {
		"@swc/core": "1.3.67",
		"@graphql-codegen/import-types-preset": "^2.2.6",
		"@graphql-codegen/introspection": "4.0.0",
		"@graphql-codegen/near-operation-file-preset": "2.5.0",
		"@graphql-codegen/schema-ast": "4.0.0"
	}
}
