{
	"name": "bleek-dot-dev",
	"version": "0.7.4",
	"type": "module",
	"author": {
		"name": "Brandon Leek",
		"url": "https://github.com/bleek42/bleek-dot-dev"
	},
	"packageManager": "pnpm",
	"engines": {
		"node": ">=18",
		"pnpm": ">=8",
		"vscode": "^1.22.0"
	},
	"scripts": {
		"dev": "next dev",
		"dev:edge": "NODE_OPTIONS='--inspect-port=9229' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"tunnel": "npm run dev && ngrok http 3000",
		"format": "prettier --write src/**/*",
		"lntfmt": "next lint src/**/*.{ts,tsx} && prettier --write src/**/*.{ts,tsx}",
		"gen": "graphql-codegen-esm --require dotenv/config --config codegen.ts",
		"gen:watch": "graphql-codegen-esm --require dotenv/config --config --watch codegen.ts",
		"gen:rm": "rm -rf src/types/graphql/gen/**/*.ts",
		"deps:rm": "rm -rf node_modules/"
	},
	"dependencies": {
		"dotenv": "16.3.1",
		"graphql": "^16.7.1",
		"graphql-request": "6.1.0",
		"graphql-tag": "2.12.6",
		"next": "^13.4.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "~5.3.11"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/import-types-preset": "^2.2.6",
		"@graphql-codegen/near-operation-file-preset": "2.5.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-codegen/typescript-graphql-request": "5.0.0",
		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@graphql-eslint/eslint-plugin": "3.20.1",
		"@styled/typescript-styled-plugin": "1.0.0",
		"@swc/core": "1.3.71",
		"@testing-library/jest-dom": "5.17.0",
		"@testing-library/react": "14.0.0",
		"@testing-library/user-event": "14.4.3",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.17",
		"@types/react-dom": "^18.2.7",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"eslint": "^8.46.0",
		"eslint-config-next": "^13.4.12",
		"eslint-config-prettier": "8.9.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-i": "2.27.5",
		"eslint-plugin-n": "16.0.1",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-react": "7.33.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"graphql-scalars": "^1.22.2",
		"prettier": "3.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"optionalDependencies": {
		"@graphql-codegen/introspection": "4.0.0",
		"@graphql-codegen/schema-ast": "4.0.0",
		"@graphql-codegen/typed-document-node": "^5.0.1",
		"@graphql-typed-document-node/core": "3.2.0"
	}
}
